        android:allowBackup="true"
        android:debuggable="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:testOnly="true"
        android:theme="@style/AppTheme" >
        <activity android:name="com.facci.josesanchezarteaga.KG2.MainActivity" />
        <activity android:name="com.facci.josesanchezarteaga.KG2.SplashsCreen" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.facci.josesanchezarteaga.KG2.Login.Registro" />
        <activity android:name="com.facci.josesanchezarteaga.KG2.Login.Login" />
        <activity android:name="com.facci.josesanchezarteaga.KG2.Servicios" />
