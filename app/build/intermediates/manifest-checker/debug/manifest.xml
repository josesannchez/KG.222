        <activity android:name="com.facci.josesanchezarteaga.KG2.Consejo" />
        <activity android:name="com.facci.josesanchezarteaga.KG2.Contacto" />
        <activity android:name="com.facci.josesanchezarteaga.KG2.vist2" />

        <provider
            android:name="com.android.tools.ir.server.InstantRunContentProvider"
            android:authorities="com.facci.josesanchezarteaga.ServicioAutomovilitico.com.android.tools.ir.server.InstantRunContentProvider"
            android:multiprocess="true" />
    </application>

</manifest>